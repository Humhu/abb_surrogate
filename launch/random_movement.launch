<launch>

    <node pkg="abb_surrogate" type="Random2DPoses.py" name="arm_tester" output="screen" required="true">
        <rosparam>
            pose_service: abb_arm/set_cartesian_linear
            seed: 0
            x_lims: [-0.4, 0.4]
            y_lims: [-0.2, 0.2]
            z_value: 0.15
            orientation: [1, 0, 0, 0]
            step_time: 2.0
            dwell_time: 2.0
            num_waypoints: 40
        </rosparam>
    </node>

</launch>